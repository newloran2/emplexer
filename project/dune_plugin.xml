<dune_plugin>
  <name>emplexer2</name>
  <caption>emplexer 2.0-B03</caption>
  <entry_points>
    <entry_point>
      <parent_media_url>root://applications</parent_media_url>
      <media_url>main</media_url>
      <caption>emplexer 2.0-B03</caption>
      <icon_url>plugin_file://icons/emplexer.aai</icon_url>
      <actions>
        <key_enter>
          <type>plugin_open_folder</type>
        </key_enter>
      </actions>
      <ip_address_required>yes</ip_address_required>
      <show_by_default>yes</show_by_default>
    </entry_point>
    <entry_point>
      <parent_media_url>root://sources</parent_media_url>
      <media_url>emplexer_section_secreen</media_url>
      <caption>emplexer 2.0-B03</caption>
      <icon_url>plugin_file://icons/emplexer.aai</icon_url>
      <actions>
        <key_enter>
          <type>plugin_open_folder</type>
        </key_enter>
      </actions>
      <ip_address_required>yes</ip_address_required>
      <show_by_default>no</show_by_default>
      <show_cookie_name>showOnMainScreen</show_cookie_name>
    </entry_point>
    <entry_point>
      <parent_media_url>setup://applications</parent_media_url>
      <media_url>configRoot</media_url>
      <caption>emplexer 2.0-B03</caption>
      <icon_url>plugin_file://icons/emplexer.aai</icon_url>
      <actions>
        <key_enter>
          <type>plugin_open_folder</type>
          </key_enter>
      </actions>
      <ip_address_required>no</ip_address_required>
      <show_by_default>yes</show_by_default>
      <show_default_value>yes</show_default_value>
      <show_cookie_name>show_tv_entry_point</show_cookie_name>
    </entry_point>
  </entry_points>
  <type>php</type>
  <params>
    <program>main.php</program>
  </params>
  <check_update>
    <schema>2</schema>
    <have_better_version>no</have_better_version>
    <url>http://dl.dropboxusercontent.com/u/5493320/emplexer/update.xml</url>
    <timeout>86400</timeout>
    <required>no</required>
  </check_update>
  <version_index>3</version_index>
  <version>2.0-B03</version>
</dune_plugin>
